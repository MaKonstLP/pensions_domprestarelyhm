<p class="filter_label">{{filter_item.name}}</p>
{% for key,item in filter_item.items %}
	<div class="filter_check {{filter_item.active[key] is defined ? '_checked':''}}" data-type="{{label}}" data-filter-checkbox-item>
		<div class="filter_check_item" data-id="{{item.id}}" data-value="{{key}}">
			<p>{{item.name}}</p>
			<span></span>
		</div>
	</div>
{% endfor %}
